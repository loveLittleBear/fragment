<?xml version="1.0" encoding="UTF-8"?>
<web-fragment version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd" metadata-complete="true">
    <!-- 指定模块名称 -->
    <name>fce-fragment</name>
    <ordering>
        <before>
            <others/>
        </before>
    </ordering>

    <servlet>
    <servlet-name>fragment</servlet-name>
    <servlet-class>com.jd.fce.fragment.core.FragmentServlet</servlet-class>
</servlet>

    <servlet-mapping>
        <servlet-name>fragment</servlet-name>
        <url-pattern>/fragment</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>fragmentView</servlet-name>
        <servlet-class>com.jd.fce.fragment.core.FragmentViewServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>fragmentView</servlet-name>
        <url-pattern>/fragmentView</url-pattern>
    </servlet-mapping>

</web-fragment>  